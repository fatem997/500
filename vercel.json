{
    "redirects": [
      { "source": "/v", "destination": "/v.html" },
      { "source": "/gh", "destination": "https://github.com/smxl/500" },
      { "source": "/mmdb", "destination": "/xb-mmdb-hh" },
      { "source": "/xb-mmdb-hh", "destination": "https://github.com/Hackl0us/GeoIP2-CN/raw/release/Country.mmdb" },
      { "source": "/x", "destination": "https://github.com/smxl/500/raw/main/500-min-cert.conf" },
      { "source": "/c", "destination": "https://github.com/smxl/500/raw/main/500-min-clash.yaml" },
      { "source": "/user", "destination": "/api/user", "permanent": false },
      { "source": "/view-source", "destination": "https://github.com/vercel/vercel" },
      {
        "source": "/:path((?!uk/).*)",
        "has": [
          {
            "type": "header",
            "key": "x-vercel-ip-country",
            "value": "GB"
          }
        ],
        "destination": "/uk/:path*",
        "permanent": false
      }
    ]
  }
